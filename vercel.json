{
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/blog/bazi-predict-true-love-timing",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/blog/bitcoin-crash-bazi-destiny",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/blog/best-bazi-wealth-chinese-astrology-blueprint",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/flying-stars",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/combination-punishment",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/travel-migration",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/day-master-career-us",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/business-success",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/pillars-analysis",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/solar-terms-energy",
      "destination": "/404",
      "permanent": true
    },
    {
      "source": "/resources/empty-branches",
      "destination": "/404",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 